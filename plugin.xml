      <source-file src="src/android/example/howen_test/SDfileExplorer.java" target-dir="src/com/example/howen_test" />
      <source-file src="src/android/example/howen_test/SerialPortMain.java" target-dir="src/com/example/howen_test" />
      <source-file src="src/android/example/howen_test/SpinnerAdapter.java" target-dir="src/com/example/howen_test" />
