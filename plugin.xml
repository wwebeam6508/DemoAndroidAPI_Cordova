      <source-file src="src/android/gen/com/example/yho_test/BuildConfig.java" target-dir="src/com/example/yho_test" />
      <source-file src="src/android/gen/com/example/yho_test/R.java" target-dir="src/com/example/yho_test" />
      <!-- jhxd -->
      <source-file src="src/android/jhxd/serial/serialService.java" target-dir="src/com/jhxd/serial" />
